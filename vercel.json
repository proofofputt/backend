{
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.12" }
    }
  ],
  "env": {
    "DATABASE_URL": "YOUR_DATABASE_URL_HERE",
    "FRONTEND_URL": "https://www.proofofputt.com"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api.py"
    }
  ]
}